{
    "manifest_version": 3,
    "name": "plants!",
    "description": "custom cursors with hand emojis!",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "images/icon-32.png"
    },
    "content_scripts": [ 
        {
            "matches": [
                "*://*/*" 
            ],
            "js": [
                "content-scripts/block.js"
            ],
            "css": [
                "content-scripts/block.css"
            ]
        }
    ]
}